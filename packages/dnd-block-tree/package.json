{
  "name": "dnd-block-tree",
  "version": "2.0.0",
  "description": "A headless React library for building hierarchical drag-and-drop interfaces",
  "author": "sandybridge",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@dnd-block-tree/react": "*"
  },
  "peerDependencies": {
    "@dnd-kit/core": "^6.0.0",
    "@dnd-kit/utilities": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18",
    "react-dom": "^18",
    "tsup": "^8.0.0",
    "typescript": "^5"
  },
  "keywords": [
    "dnd",
    "drag-and-drop",
    "block",
    "tree",
    "react",
    "dnd-kit",
    "headless",
    "hierarchical"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thesandybridge/dnd-block-tree.git"
  },
  "bugs": {
    "url": "https://github.com/thesandybridge/dnd-block-tree/issues"
  },
  "homepage": "https://github.com/thesandybridge/dnd-block-tree#readme"
}
