{
  "name": "@dnd-block-tree/vanilla",
  "version": "2.0.0",
  "description": "Vanilla JS/TS adapter for dnd-block-tree â€” zero framework dependencies",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-block-tree/core": "^2.0.0"
  },
  "devDependencies": {
    "jsdom": "^25.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thesandybridge/dnd-block-tree",
    "directory": "packages/vanilla"
  },
  "license": "MIT",
  "keywords": [
    "dnd",
    "drag-and-drop",
    "block",
    "tree",
    "vanilla",
    "headless",
    "hierarchical"
  ]
}
